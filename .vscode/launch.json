    "version": "0.2.0",
    "configurations": [
        {
            "name": "DEBUG: Current",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {"PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2", "JAX_TRACEBACK_FILTERING": "off", "PYDEVD_WARN_EVALUATION_TIMEOUT": "30"}
        },
        {
            "name": "DEBUG: Main",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exo/main.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {"PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2", "JAX_TRACEBACK_FILTERING": "off", "PYDEVD_WARN_EVALUATION_TIMEOUT": "30"}
        }
    ]
}